generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model user {
  id       Int    @id @default(autoincrement())
  username String @db.VarChar(255)
  password String @db.VarChar(255)
  name     String @db.VarChar(255)
  lastname String @db.VarChar(255)
}
